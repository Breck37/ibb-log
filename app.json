{
  "expo": {
    "name": "ibb-log",
    "slug": "ibb-log",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ibblog",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ibblog.app",
      "infoPlist": {
        "NSCameraUsageDescription": "IBB Log needs camera access to take workout photos.",
        "NSPhotoLibraryUsageDescription": "IBB Log needs photo library access to attach workout images.",
        "NSMicrophoneUsageDescription": "IBB Log needs microphone access for voice-to-text comments.",
        "NSSpeechRecognitionUsageDescription": "IBB Log uses speech recognition for voice-to-text comments.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ibblog.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5bf887e1-ee59-4416-b443-8cfc8a7e4896"
      }
    },
    "owner": "bdotdev"
  }
}
